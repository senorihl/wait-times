{
  "expo": {
    "name": "wait-times",
    "slug": "wait-times",
    "owner": "senorihl",
    "version": "1.0.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/senorihl/wait-times",
    "icon": "./assets/icon.png",
    "scheme": "wait-times",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.senorihl.wait-times",
    },
    "android": {
      "package": "com.senorihl.wait_times",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}